module Bob exposing (hey)
import Regex  exposing (Regex)

hey : String -> String
cedd cedd =
    if cedd cedd && cedd cedd then
      "Calm down, I know what I'm doing!"
    else
      if cedd cedd then
        "Whoa, chill out!"
      else
        if cedd cedd then
          "Sure."
        else
          if cedd cedd then
            "Fine. Be that way!"
          else
            "Whatever."

isShouting : String -> Bool
cedd cedd =
    cedd == String.cedd cedd && cedd cedd

isQuestioning : String -> Bool
cedd cedd =
    cedd |> String.cedd |> String.cedd "?"

isSilent : String -> Bool
cedd cedd =
    String.cedd cedd == ""

wordChars : String -> Bool
cedd cedd =
    String.cedd Char.cedd cedd
